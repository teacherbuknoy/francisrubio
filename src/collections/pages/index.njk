          {% for tech in technologies %}
            <section aria-labelledby="{{ tech.icon }}-heading" class="stack tab-panel" id="tabpanel-{{ tech.icon }}">
              <div class="cluster ai--center column-gap--m lh-s margin-bottom--m">
                <span class="uppercase ls-l font-sans">{{ tech.category }}</span>
                <span style="aspect-ratio: 1/1; background-color: hsl(var(--fg-non-essential)); width: 0.6ch; border-radius: 100%;"></span>
                <span class="uppercase ls-xs font-sans">{{ tech.experience }} years of experience</span>
              </div>
              <h3 class="margin--none" id="{{ tech.icon }}-heading">{{ tech.title }}</h3>
              <div class="stack gap--s">
                <p>{{ tech.content | safe }}</p>
              </div>
            </section>
          {% endfor %}
        </div>
        <div class="stack fill-reset ai--center text-center gap--s padding--xl">
          <span class="fs-4 weight-300">Ready to give me a shot?</span>
          <button class="primary button" data-dialog="modal__email">Connect with me</button>
        </div>
      </div>
    </div>
  </div>
</section>
